#include <dt-bindings/gpio/tegra234-gpio.h>
/ {
	fixed-regulators {
		battery_reg: regulator@0 {
			compatible = "regulator-fixed";
			reg = <0>;
			regulator-name = "vdd-ac-bat";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};
		vdd_12v_sys: regulator@100 {
			compatible = "regulator-fixed";
			reg = <100>;
			regulator-name = "vdd-12v-sys";
			regulator-min-microvolt = <12000000>;
			regulator-max-microvolt = <12000000>;
			regulator-always-on;
		};
		vdd_5v_sys: regulator@101 {
			compatible = "regulator-fixed";
			reg = <101>;
			regulator-name = "vdd-5v-sys";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
		};
		vdd_3v3_sys: regulator@102 {
			compatible = "regulator-fixed";
			reg = <102>;
			regulator-name = "vdd-3v3-sys";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			regulator-always-on;
		};
		vdd_1v8_sys: regulator@103 {
			compatible = "regulator-fixed";
			reg = <103>;
			regulator-name = "vdd-1v8-sys";
			regulator-min-microvolt = <1800000>;
			regulator-max-microvolt = <1800000>;
			regulator-always-on;
		};
		vdd_cam_2v8: regulator@106 {
			compatible = "regulator-fixed";
			reg = <106>;
			regulator-name = "vdd-cam-2v8";
			regulator-min-microvolt = <2800000>;
			regulator-max-microvolt = <2800000>;
			regulator-always-on;
		};
		vbusA_cvb: regulator@108 {
			compatible = "regulator-fixed";
			reg = <108>;
			regulator-name = "vbusA-cvb";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
			vin-supply = <&vdd_5v_sys>;
		};
		vbusB_cvb: regulator@109 {
			compatible = "regulator-fixed";
			reg = <109>;
			regulator-name = "vbusB-cvb";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			regulator-always-on;
			vin-supply = <&vdd_5v_sys>;
		};
		vdd_3v3_sd: regulator@111 {
			compatible = "regulator-fixed";
			reg = <111>;
			regulator-name = "vdd-3v3-sd";
			regulator-min-microvolt = <3300000>;
			regulator-max-microvolt = <3300000>;
			vin-supply = <&vdd_3v3_sys>;
			gpio = <&tegra_main_gpio TEGRA234_MAIN_GPIO(A, 0) 0>;
			regulator-always-on; /* keeping always on for bring up */
			enable-active-high;
		};
		vdd_usb_vbus0: regulator@112 {
			compatible = "regulator-fixed";
			reg = <112>;
			regulator-name = "vdd-usb-vbus0";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vdd_5v_sys>;
			gpio = <&tegra_main_gpio TEGRA234_MAIN_GPIO(I, 1) 0>;
			enable-active-high;
		};
		vdd_usb_vbus1: regulator@113 {
			compatible = "regulator-fixed";
			reg = <113>;
			regulator-name = "vdd-usb-vbus1";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			vin-supply = <&vdd_5v_sys>;
			gpio = <&tegra_main_gpio TEGRA234_MAIN_GPIO(I, 2) 0>;
			enable-active-high;
		};
	};
};
