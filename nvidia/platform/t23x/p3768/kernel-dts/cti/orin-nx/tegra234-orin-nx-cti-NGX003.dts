#include "tegra234-orin-nx-cti-base.dtsi"

/{
    nvidia,dtsfilename = "tegra234-orin-nx-cti-NGX003.dts";

    //enable serial connections
    

    pwm_fan_shared_data: pfsd {
        pwm_gpio = <&tegra_main_gpio TEGRA234_MAIN_GPIO(H, 0) GPIO_ACTIVE_LOW>; //this looks to be tach
    };
    spi@3210000{ /* SPI1 in 40 pin conn */
        status = "disabled";
    };

    spi@3230000{ /* SPI3 in 40 pin conn */
        status = "disabled";
    };

    mttcan@c310000{
        status = "disabled";
    };
    /**define tca9539 i2c-0*/
    i2c@c240000 {
        status = "okay";

        cam_gpios:tca9539@74{
            status = "okay";
            compatible="ti,tca9539";
            vcc-supply = <&vdd_1v8_sys>;
            gpio-controller;
            #gpio-cells=<2>;
            reg=<0x74>;
        };
    };

    /** define tlc59116PWR on i2c-1*/
    i2c@c250000 {
        status = "okay";
        rgb_leds:tlc59116@60{
            status = "okay";
            compatible="ti,tlc59116";
            #address-cells = <1>;
            #size-cells = <0>;
            reg=<0x60>;

            red@0 {
                label="red";
                reg = <0x0>;
            };
            green@1 {
                label="green";
                reg = <0x1>;
            };
            blue@2 {
                label="blue";
                reg = <0x2>;
            };

        };
    };

    /** required for i2c pins on P9 header*/
    i2c@3180000 {
        tca9546@70 {
            compatible = "nxp,pca9544";
            reg = <0x70>;
            #address-cells = <1>;
            #size-cells = <0>;
            //skip_mux_detect = "yes";
            vif-supply = <&vdd_1v8_sys>;
            vcc-supply = <&vdd_1v8_sys>;
            vcc_lp = "vcc";
        };
    };


};

