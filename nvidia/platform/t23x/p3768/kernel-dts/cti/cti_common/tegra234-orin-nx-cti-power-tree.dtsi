#include "tegra234-orin-nx-cti-regulator.dtsi"

//defualt regulators for nx boards defined here

/{
    bpmp {
        i2c {
            vrs@3c {
                //Power Sequence RTC
                rtc {
                    status = "okay";
                };
            };
        };
    };
};

&xusb_padctl{
    
    ports {
        usb2-0 {
            vbus-supply = <&cti_usb_vbus>;
        };
        usb2-1 {
            vbus-supply = <&vdd_av10_hub>;
        };
        usb2-2 {
            vbus-supply = <&vdd_5v_sys>;
        };
    };
};
